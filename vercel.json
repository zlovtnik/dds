{
    "version": 2,
    "buildCommand": "./vercel-build.sh",
    "outputDirectory": "api",
    "functions": {
        "api/index": {
            "runtime": "provided",
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index",
            "graphiql": "/graphql"
        }
    ],
    "env": {
        "RUST_BACKTRACE": "1",
        "RUST_LOG": "info",
        "SQLX_OFFLINE": "true"
    },
    "build": {
        "env": {}
    }
}