{
    "version": 2,
    "buildCommand": "./vercel-build.sh",
    "outputDirectory": "api",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.sh"
        },
        {
            "src": "/graphiql",
            "dest": "/api/index.sh"
        },
        {
            "src": "/graphql",
            "dest": "/api/index.sh"
        }
    ],
    "env": {
        "RUST_BACKTRACE": "1",
        "RUST_LOG": "info",
        "SQLX_OFFLINE": "true"
    },
    "build": {
        "env": {}
    }
}