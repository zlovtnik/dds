{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.rs",
            "use": "vercel-rust@4.0.8"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/graphql",
            "dest": "/api/graphql.rs"
        },
        {
            "src": "/graphiql",
            "dest": "/api/graphql.rs"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "env": {
        "RUST_BACKTRACE": "1",
        "RUST_LOG": "info",
        "SQLX_OFFLINE": "false"
    }
}